@{
    ViewBag.Title = "Welcome";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div>
       <ul>
           @for(int i=0;i<ViewBag.times;i++)
           {
               <li>@ViewBag.message</li>
           }
       </ul>
    </div>
</body>
</html>
